package com.itwill.member;

public class Member {

	private String id;//아이디
	private String passwd;//비번
	private String name;//이름
	private String email1;//이메일주소
	private String email2;//이메일뒤에주소
	

	public Member() {
		// TODO Auto-generated constructor stub
	}

	

	public Member(String id, String passwd, String name, String email1,
			String email2) {
		super();
		this.id = id;
		this.passwd = passwd;
		this.name = name;
		this.email1 = email1;
		this.email2 = email2;
	}



	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getPasswd() {
		return passwd;
	}

	public void setPasswd(String passwd) {
		this.passwd = passwd;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getEmail1() {
		return email1;
	}

	public void setEmail1(String email1) {
		this.email1 = email1;
	}

	public String getEmail2() {
		return email2;
	}

	public void setEmail2(String email2) {
		this.email2 = email2;
	}


	public boolean isMatchPassword(String passwd){
		boolean isMatch=false;
		if(this.passwd.equals(passwd)){
			isMatch=true;
		}
		return isMatch;
	}
	
}


