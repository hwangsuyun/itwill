package basket;

/*
  CREATE TABLE SCOTT.BASKET 
(
  BASKET_NO      NUMBER		        NOT NULL, 
  BASKET_DATE    DATE               NOT NULL, 
  ITEM_CODE      NVARCHAR2(100)     NOT NULL, 
  ITEM_OPTION    NVARCHAR2(100)     NOT NULL, 
  ITEM_PRICE     NUMBER             NOT NULL, 
  ITEM_CNT       NUMBER             NOT NULL, 
  MEMBER_ID      NVARCHAR2(100)     NOT NULL
)

 */
public class BasketDTO 
{
	private int basket_no;
	private String basket_date;
	private String item_code;
	private String item_option;
	private int item_price;
	private int item_cnt;
	private String member_id;
	
	public BasketDTO() { }
	
	public BasketDTO(
			String item_code, String item_option, int item_price,
			int item_cnt, String member_id
		   ) 
	{
		this.item_code = item_code;
		this.item_option = item_option;
		this.item_price = item_price;
		this.item_cnt = item_cnt;
		this.member_id = member_id;
	}

	public BasketDTO(
							int basket_no, String basket_date,
							String item_code, String item_option, int item_price,
							int item_cnt, String member_id
						   ) 
	{
		this.basket_no = basket_no;
		this.basket_date = basket_date;
		this.item_code = item_code;
		this.item_option = item_option;
		this.item_price = item_price;
		this.item_cnt = item_cnt;
		this.member_id = member_id;
	}

	public int getBasket_no() 
	{
		return basket_no;
	}

	public void setBasket_seq(int basket_seq)
	{
		this.basket_no = basket_seq;
	}


	public String getBasket_date() 
	{
		return basket_date;
	}

	public void setBasket_date(String basket_date)
	{
		
		this.basket_date = basket_date;
	}

	public String getItem_code() 
	{
		return item_code;
	}

	public void setItem_code(String item_code)
	{
		this.item_code = item_code;
	}

	public String getItem_option()
	{
		return item_option;
	}

	public void setItem_option(String item_option) 
	{
		this.item_option = item_option;
	}

	public int getItem_price() 
	{
		return item_price;
	}

	public void setItem_price(int item_price)
	{
		this.item_price = item_price;
	}

	public int getItem_cnt() 
	{
		return item_cnt;
	}

	public void setItem_cnt(int item_cnt) 
	{
		this.item_cnt = item_cnt;
	}

	public String getMember_id() 
	{
		return member_id;
	}

	public void setMember_id(String member_id) 
	{
		this.member_id = member_id;
	}
	
	
	
}













