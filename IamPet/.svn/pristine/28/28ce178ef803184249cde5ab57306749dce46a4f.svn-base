function over(i)
{
	imgOut();
	document.getElementById("menu"+i).style.display = "block";
}

function imgOut()
{
	var subCount = document.getElementById("main_navi").getElementsByTagName("a");

	for(var i = 0; i < subCount.length; i++)
	{
		document.getElementById("menu" + i).style.display = "none";
	}
}