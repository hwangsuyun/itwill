/* PRIMARY LAYOUT ELEMENTS   
----------------------------------------------------------*/

/* you can specify a greater or lesser percentage for the 
page width. Or, you can specify an exact pixel width. */
.page {
    width: 800px;
    margin-left: auto;
    margin-right: auto;
}

header, #header {
    position: relative;
    height: 133px;
    margin-bottom: 0px;
    padding: 0;
    background-image: url("../image/top_bg.gif");
    background-repeat: repeat-x;
    
    margin-left: auto;
    margin-right: auto;
}

header h1, #header h1 {
    font-weight: bold;
    padding: 0px 0;
    margin: 0;
    color: #fff;
    border: none;
    line-height: 1.8em;
    font-size: 32px !important;
    text-shadow: 1px 1px 2px #111;
}

#header .title{
	padding-top: 10px;
	height: 89px;
}
	

#main {
    padding: 30px 30px 15px 30px;
    background-color: #fff;
    border-radius: 2px 2px 0px 0px;
    -webkit-border-radius: 2px 2px 0px 0px;
    -moz-border-radius: 2px 2px 0px 0px;
}


footer, 
#footer {
    background-color: #fff;
    color: #999;
    padding: 10px 0;
    text-align: center;
    line-height: normal;
    margin: 0 0 30px 0;
    font-size: .9em;
    border-radius: 0 0 2px 2px;
    -webkit-border-radius: 0 0 2px 2px;
    -moz-border-radius: 0 0 2px 2px;
}

/* FORM LAYOUT ELEMENTS   
----------------------------------------------------------*/

fieldset {
    border: 1px solid #ddd;
    padding: 0 1.4em 1.4em 1.4em;
    margin: 0 0 1.5em 0;
}

legend {
    font-size: 1.2em;
    font-weight: bold;
}

textarea {
    min-height: 75px;
}

input[type="text"], 
input[type="password"] {
    border: 1px solid #ccc;
    padding: 1px;
    font-size: 1.0em;
    color: #444;
    height: 19px;
    /*width: 150px;*/
}

select {
    border: 1px solid #ccc;
    padding: 2px;
    font-size: 1.0em;
    color: #444;
}

input[type="submit"] {
    font-size: 1.0em;
}

/* TABLE
----------------------------------------------------------*/

table {
    border: solid 1px #e8eef4;
    border-collapse: collapse;
}

table td {
    padding: 5px;
    border: solid 1px #e8eef4;
}

table th {
    padding: 6px 5px;
    text-align: left;
    background-color: #e8eef4;
    border: solid 1px #e8eef4;
}

