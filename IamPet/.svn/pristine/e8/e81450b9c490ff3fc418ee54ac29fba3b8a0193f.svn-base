/* BOARD TABLE
----------------------------------------------------------*/
h5{
	padding: 0px;
	margin: 0px;
}
.board_list th{
    padding: 8px 5px;
    text-align: center;
}
.board_list tr{
    text-align: center;
}
.board_list #td_title{
	text-align: left;
}
.board_list .number{
	width: 50px;
}
.board_list .title{
	width: 250px;
}

.board_list .writer{
	width: 100px;
}

.board_list .date{
	width: 150px;
}

.board_list .count{
	width: 50px;
}
.board_list .ip{
	width: 100px;
}
.board_menu{
	margin-top: 5px;
	margin-bottom: 0px;
	width: 100%;
	display: table;
}
.board_left_menu{
	text-align: left;
	width: 50%;
	display: table-cell;
}
.board_right_menu{
	width: 50%;
	text-align: right;
	display: table-cell;
}
.board_group{
	text-align: center;
}
/* Popup---------------------------*/
.alphaPopup{
	position: absolute;
	top:0px;
	left:0px;
	width:100%;
	height:100%;
	filter: alpha(opacity=60); 
	-khtml-opacity: 0.6; 
	-moz-opacity:0.6; 
	opacity: 0.6;
	background-color:#000000;
	z-index: 2;
}
.deletePopup{
	position:absolute;
	top:50%;
	left:50%;
	margin-top:-100px;
	margin-left:-100px;
	width:200px;
	height:150px;
	padding: 20px;
	background-color: #fff;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    z-index: 3;
}
.updatePopup{
	position:absolute;
	top:50%;
	left:50%;
	margin-top:-100px;
	margin-left:-100px;
	width:200px;
	height:100px;
	padding: 20px;
	background-color: #fff;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    z-index: 3;
}

