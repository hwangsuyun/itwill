/* TAB MENU   
----------------------------------------------------------*/
ul#menu {
    /*border-bottom: 1px #5C87B2 solid;
    padding: 0 0 1px;*/
    position: relative;
	font-size: 9pt;
    margin: 0;
    text-align: right;
    color: #787878;
}

ul#menu li {
    display: inline;
    list-style: none;
	padding-right: 5px;
	padding-left: 5px;
}

ul#menu li#greeting {
    font-weight: bold;
    text-decoration: none;
    color: #fff;
}

ul#menu li a {
    font-weight: normal;
    text-decoration: none;
    background-color: #fff;
    color: #787878;
    border-radius: 4px 4px 4px 4px;
    -webkit-border-radius: 4px 4px 4px 4px;
    -moz-border-radius: 4px 4px 4px 4px;
}

ul#menu li a:hover {
    background-color: #fff;
    color: #cdcdcd;
    text-decoration: none;
}

ul#menu li a:active {
    /*background-color: #a6e2a6;*/
    text-decoration: none;
}

ul#menu li.selected a {
    background-color: #fff;
    color: #000;
}


