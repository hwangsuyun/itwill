a{ selector-dummy : expression(this.hideFocus=true); }

a:link {
    color: #034af3;
    text-decoration: none;
}

a:visited {
    color: #505abc;
    text-decoration: none;
}

a:hover {
    color: #1d60ff;
    text-decoration: underline;
}

