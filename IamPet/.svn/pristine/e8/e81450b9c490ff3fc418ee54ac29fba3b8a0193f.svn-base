body {
    background-color: #fff;
    font-size: .80em;
    font-family: "Trebuchet MS", Verdana, Helvetica, Sans-Serif;
    margin: 0;
    padding: 0;
    color: #696969;
    line-height: 1.20em;
}

header,
footer,
nav,
section {
    display: block;
}

